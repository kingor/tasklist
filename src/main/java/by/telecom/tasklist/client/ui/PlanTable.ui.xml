<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.radius {
		padding: 10px;
		border: 5px solid #ADD8E6;
		border-radius: 5px;
		border-spacing: 10px;
		font-size: 5pt;
	}
	.marginTable {
		margin: 1px 4px 4px 0px;

	}
	</ui:style>
	<g:DockLayoutPanel unit="EM" styleName="{style.radius}">
		<g:center>
			<g:FlexTable borderWidth="1" ui:field="planTable" styleName="{style.marginTable}"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 